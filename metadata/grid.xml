<?xml version="1.0"?>
<wayfire>
	<plugin name="grid">
	<_short>Grid</_short>
	<category>Window Management</category>
	<option name="duration" type="int">
		<_short>Duration</_short>
		<default>300</default>
	</option>
	<option name="slot_bl" type="key">
		<_short>Bottom Left</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP1</default>
	</option>
	<option name="slot_b" type="key">
		<_short>Bottom</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP2</default>
	</option>
	<option name="slot_br" type="key">
		<_short>Bottom Right</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP3</default>
	</option>
	<option name="slot_l" type="key">
		<_short>Left</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP4</default>
	</option>
	<option name="slot_c" type="key">
		<_short>Center</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP5</default>
	</option>
	<option name="slot_r" type="key">
		<_short>Right</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP6</default>
	</option>
	<option name="slot_tl" type="key">
		<_short>Top Left</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP7</default>
	</option>
	<option name="slot_t" type="key">
		<_short>Top</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP8</default>
	</option>
	<option name="slot_tr" type="key">
		<_short>Top Right</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_KP9</default>
	</option>
	</plugin>
</wayfire>
