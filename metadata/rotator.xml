<?xml version="1.0"?>
<wayfire>
	<plugin name="rotator">
	<_short>Rotator</_short>
	<category>Desktop</category>
	<option name="rotate_up" type="key">
		<_short>Rotate Up</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_UP</default>
	</option>
	<option name="rotate_down" type="key">
		<_short>Rotate Down</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_DOWN</default>
	</option>
	<option name="rotate_left" type="key">
		<_short>Rotate Left</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_LEFT</default>
	</option>
	<option name="rotate_right" type="key">
		<_short>Rotate Right</_short>
		<default>&lt;ctrl&gt; &lt;alt&gt; KEY_RIGHT</default>
	</option>
	</plugin>
</wayfire>
